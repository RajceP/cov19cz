{
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.organizeImports",
    "source.fixAll.stylelint",
    "source.fixAll.eslint"
  ],
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "*.js": "javascriptreact"
  },
  "files.associations": {
    "*.js": "javascriptreact"
  }
}

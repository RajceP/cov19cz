{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "pwa-chrome",
      "runtimeExecutable": "/Applications/Brave Browser Nightly.app/Contents/MacOS/Brave Browser Nightly",
      "preLaunchTask": "npm: start",
      "postDebugTask": "npm stop",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
